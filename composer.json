{
  "name": "lg33/spinupwp-composer-site",
  "keywords": [
    "WordPress",
    "Composer",
    "SpinupWP"
  ],
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Louis Gaillard",
      "homepage": "https://lgaillard.fr/"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/lg33/wp-paheko.git"
    },
    {
      "type": "package",
      "package": {
        "name": "kd2/kd2fw",
        "version": "7.2",
        "dist": {
          "url": "https://fossil.kd2.org/kd2fw/zip/39b1f93501c6c58597b18f9d562ebc2e50e24b84/kd2.zip",
          "type": "zip"
        },
        "autoload": {
            "psr-4": {
                "KD2\\": "src/lib/KD2/"
            }
        }
      }
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.4",
    "johnpbloch/wordpress": "^6.7",
    "lg33/wp-paheko": "^1.3",
    "oscarotero/env": "^1.1.0",
    "vlucas/phpdotenv": "^5.3",
    "wpackagist-plugin/limit-login-attempts-reloaded": "*",
    "wpackagist-plugin/spinupwp": "*",
    "wpackagist-theme/twentytwentyfive": "^1.0"
  },
  "extra": {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "autoload": {
    "psr-4": {"DeliciousBrains\\SpinupWPComposerSite\\": "app/"}
  }
}
